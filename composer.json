{
    "name": "uvptl/depend",
    "description": "Dependency injector for bootstrapping object-oriented PHP applications.",
    "keywords": ["dependency injection", "dic", "ioc"],
    "license": "MIT",
    "authors": [
        {
            "name": "Umesh Patel",
            "email": "umesh@archisys.in",
            "role": "Creator / Main Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "fabpot/php-cs-fixer": "~1.9",
        "athletic/athletic": "~0.1"
    },
    "autoload": {
        "psr-4": {
            "Auryn\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Auryn\\Test\\": "test/"
        },
        "files": ["test/fixtures.php"]
    }
}
